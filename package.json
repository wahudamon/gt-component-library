{
  "name": "gamatechno-vue-component-library",
  "version": "0.0.26",
  "description": "Gamatechno UI component library built using vue",
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "storybook": "start-storybook -p 6006",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "test:unit": "vue-cli-service test:unit",
    "build:css": "sass --update scss:css --style compressed",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:storybook": "build-storybook",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "prebuild": "rimraf ./dist"
  },
  "main": "dist/gamatechno-vue-component-library.ssr.js",
  "module": "dist/gamatechno-vue-component-library.esm.js",
  "browser": "dist/gamatechno-vue-component-library.esm.js",
  "unpkg": "dist/gamatechno-vue-component-library.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "css/**/*.css",
    "css/**/*.css.map",
    "src/lib/*"
  ],
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/vue": "^6.5.9",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "~4.5.15",
    "@vue/cli-service": "^4.5.13",
    "@vue/test-utils": "^1.3.0",
    "babel-loader": "^8.2.3",
    "cache-loader": "^4.1.0",
    "core-js": "^3.21.1",
    "cross-env": "^7.0.3",
    "css-loader": "^3.6.0",
    "minimist": "^1.2.5",
    "node-sass": "^6.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "sass-loader": "^10.0.5",
    "vue": "^2.6.14",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  },
  "peerDependencies": {
    "bootstrap": "^5.1.3"
  },
  "engines": {
    "node": ">=12"
  },
  "sideEffects": false
}
