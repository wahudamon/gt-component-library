{
  "name": "gamatechno-vue-component-library",
  "version": "0.0.20",
  "description": "Gamatechno UI component library built using vue",
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "storybook": "start-storybook -p 6006",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "test:unit": "vue-cli-service test:unit",
    "watch:css": "sass --watch scss:css",
    "build:css": "sass --update scss:css",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:storybook": "build-storybook",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "prebuild": "rimraf ./dist",
    "chromatic": "npx chromatic --project-token=9b7b0878e638"
  },
  "main": "dist/gamatechno-vue-component-library.ssr.js",
  "module": "dist/gamatechno-vue-component-library.esm.js",
  "browser": "dist/gamatechno-vue-component-library.esm.js",
  "unpkg": "dist/gamatechno-vue-component-library.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "css/*.css",
    "css/*.css.map",
    "src/lib/*"
  ],
  "dependencies": {
    "bootstrap": "^5.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addons": "^6.4.19",
    "@storybook/theming": "^6.4.19",
    "@storybook/vue": "^6.4.19",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "~4.5.15",
    "@vue/cli-service": "^4.5.13",
    "@vue/test-utils": "^1.3.0",
    "babel-loader": "^8.2.3",
    "chromatic": "^6.5.3",
    "cross-env": "^7.0.3",
    "js-beautify": "^1.14.3",
    "minimist": "^1.2.6",
    "node-sass": "^5.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "sass-loader": "^10.0.5",
    "vue": "^2.6.14",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "bootstrap": "^5.1.3"
  },
  "engines": {
    "node": ">=12"
  },
  "sideEffects": false
}
